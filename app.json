{
    "expo": {
        "name": "Ryde",
        "slug": "ryde",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "uberclone",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#2F80ED"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.yopijay.ryde",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyAexatwRIyEaZ1Gm8pIUtz2p65Sxr04X10"
                }
            },
            "package": "com.yopijay.ryde",
            "permissions": [
                "ACCESS_FINE_LOCATION",
                "ACCESS_COARSE_LOCATION",
                "INTERNET"
            ]
        },
        "web": {
            "bundler": "metro",
            "output": "server",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            [
                "expo-router",
                {
                    "origin": "https://ryde.com/"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "GOOGLE_MAPS_API_KEY": "AIzaSyAexatwRIyEaZ1Gm8pIUtz2p65Sxr04X10",
            "router": {
                "origin": "https://ryde.com/"
            },
            "eas": {
                "projectId": "5e9038fc-6270-4bd7-923f-6df064c91a65"
            }
        }
    }
}
